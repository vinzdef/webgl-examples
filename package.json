{
  "name": "webgl-programming-guide",
  "version": "1.0.0",
  "description": "A static website boilerplate",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git@git.caffeina.co:lorenzo.girardi/silvietta.git"
  },
  "scripts": {
    "dev": "gulp dev",
    "build": "gulp build"
  },
  "contributors": [
    "Vincent De Feo <vincent.defeo@caffeina.com> (http://ghzmdr.github.io/)"
  ],
  "license": "MIT",
  "browserslist": [
    "last 3 major versions",
    "Safari >= 9",
    "iOS >= 9",
    "Android >= 6.0",
    "Opera > 30",
    "Edge >= 12",
    "Explorer 11"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/**/*.{js}",
      "server/**/*.{js}",
      "!**/node_modules/**"
    ],
    "roots": [
      "client/",
      "server/"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/public/js/*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/public/css/*.css",
      "maxSize": "30 kB"
    }
  ],
  "dependencies": {
    "@okiba/component": "^1.0.4",
    "@okiba/dom": "^1.0.4",
    "@okiba/math": "^1.0.9",
    "golc": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.27.10",
    "browserslist": "^4.5.4",
    "bundlesize": "0.15.x",
    "globby": "^8.0.1",
    "glslify-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nunjucks": "^4.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-error": "^1.0.5",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "lazypipe": "^1.0.1",
    "nunjucks": "^3.1.3",
    "raw-loader": "^2.0.0",
    "sass-mq": "^5.0.0",
    "webpack": "^4.20.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
